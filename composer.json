{
    "name": "raindrop/routing-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Raindrop RoutingBundle",
    "keywords": ["routing", "router", "database", "doctrine"],
    "homepage": "https://github.com/raindropdevs",
    "license": "MIT",
    "authors": [
        {
            "name": "Raindrop Dev Team",
            "homepage": "https://github.com/raindropdevs/RaindropRoutingBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/monolog-bundle": ">=2.1",
        "doctrine/doctrine-bundle": "^2.4"
    },
    "require-dev": {
        "symfony/class-loader": "~2.2",
        "symfony/yaml": "~2.2",
        "symfony/form": "~2.2",
        "symfony/finder": "~2.2"
    },
    "autoload": {
        "psr-4": { "Raindrop\\RoutingBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
